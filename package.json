{
  "name": "rslh-filter-gen",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build -w packages/core && npm run build -w packages/cli && npm run build -w packages/web",
    "test": "vitest run",
    "lint": "eslint .",
    "dev": "npm run dev --workspace=packages/web",
    "clean": "rm -rf packages/*/dist packages/*/*.tsbuildinfo"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@fast-check/vitest": "^0.2.4",
    "eslint": "^9.0.0",
    "fast-check": "^4.5.3",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^3.0.0"
  }
}
